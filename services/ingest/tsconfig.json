{
    "extends":  "../../tsconfig.json",
    "compilerOptions":  {
                            "tsBuildInfoFile":  "./node_modules/.tmp/tsconfig.ingest.tsbuildinfo",
                            "rootDir":  "./src",
                            "outDir":  "./dist",
                            "target":  "ES2020",
                            "module":  "ESNext",
                            "moduleResolution":  "bundler",
                            "strict":  true,
                            "esModuleInterop":  true,
                            "allowSyntheticDefaultImports":  true,
                            "skipLibCheck":  true,
                            "resolveJsonModule":  true,
                            "sourceMap":  true
                        },
    "include":  [
                    "src/**/*.ts"
                ],
    "exclude":  [
                    "dist",
                    "node_modules"
                ]
}
